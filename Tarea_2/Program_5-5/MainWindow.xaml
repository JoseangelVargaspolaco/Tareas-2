        <Border BorderBrush="Black" BorderThickness="1" Grid.ColumnSpan="2"/>

        <TextBlock HorizontalAlignment="Left" Margin="20,53,10,0" Text="Ingrese un numero" VerticalAlignment="Top" FontSize="16"/>
    
